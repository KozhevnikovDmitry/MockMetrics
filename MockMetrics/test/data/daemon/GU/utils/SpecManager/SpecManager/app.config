    <add key="TestConnectionConfig" value="Test" />
    <add key="WorkConnectionConfig" value="Work" />

    <!--Шаблон подключения к базе Postgre-->
    <add key="PostgreConnectionStringTemplate" value="Server={0};Port={1};Database={2};User Id={3};Password={4};" />
    
