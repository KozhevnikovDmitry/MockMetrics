    <!--Настройки подключения к тестовой базе Postgre-->
    <add key="TestPostgreServer" value="172.25.253.154" />
    <add key="TestPostgrePort" value="5432" />
    <add key="TestPostgreDatabase" value="gosus" />
    <add key="TestPostgreUser" value="gosus" />
