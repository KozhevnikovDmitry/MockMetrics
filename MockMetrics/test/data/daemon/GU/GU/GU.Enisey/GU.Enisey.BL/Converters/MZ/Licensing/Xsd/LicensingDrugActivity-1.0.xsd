<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:krsk="urn://x-artefacts-it-ru/dob/state-services/krsk/LicensingDrugActivity/1.0" xmlns:commons="urn://x-artefacts-it-ru/dob/state-services/commons/1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:hints="urn://x-artefacts-it-ru/dob/xml-editor/rendering-hints/1.1" xmlns:attach="http://smev.gosuslugi.ru/request/rev111111" targetNamespace="urn://x-artefacts-it-ru/dob/state-services/krsk/LicensingDrugActivity/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="urn://x-artefacts-it-ru/dob/state-services/commons/1.1" schemaLocation="state-services-commons-1.1.xsd"/>
	<xs:import namespace="http://smev.gosuslugi.ru/request/rev111111" schemaLocation="smev-attachments-2.2.4.xsd"/>
	<xs:element name="LicensingDrugActivity">
		<xs:annotation>
			<xs:documentation>Лицензирование деятельности, связанной с оборотом нароктических средств</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice hints:rus="Выберите вариант оказания услуги" hints:showInChoiceScreen="true">
				<xs:element name="NewLicense" type="krsk:OrgInfoType">
					<xs:annotation>
						<xs:documentation>Получение лицензии на осуществление деятельности по обороту наркотических средств</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Reregistration">
					<xs:annotation>
						<xs:documentation>Переоформление лицензии на осуществление деятельности по обороту наркотических средств</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="OrgInfo" type="krsk:OrgInfoType">
								<xs:annotation>
									<xs:documentation>Информация о лицензиате, его правопреемнике, либо об ином предусмотренном федеральным законом лице</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LicenseInfo" type="krsk:LicenseInfoType">
								<xs:annotation>
									<xs:documentation>Информация о переоформляемой лицензии на осуществление деятельности по обороту наркотических средств</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Base" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Основание для переоформления лицензии</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice hints:rus="Выберите основание для переоформления лицензии" hints:showInChoiceScreen="true">
										<xs:element name="Reorganization">
											<xs:annotation>
												<xs:documentation>Реорганизация юридического лица в форме преобразования</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="FullName">
														<xs:annotation>
															<xs:documentation>Полное наименование юридического лица</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="LawForm">
														<xs:annotation>
															<xs:documentation>Организационно-правовая форма юридического лица</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="ShortName" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сокращенное наименование юридического лица  (в случае, если имеется)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="FirmName" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Фирменное наименование юридического лица (в случае, если имеется)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="OGRN" hints:rus="Государственный регистрационный номер записи о создании юридического лица (ОГРН)">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="[1;2;5]{1}\d{12}"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="DocData" type="krsk:EGRULDataType">
														<xs:annotation>
															<xs:documentation>Данные документа, подтверждающего факт внесения соответствующих изменений в единый государственный реестр юридических лиц</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="INN" hints:rus="ИНН юридического лица">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{8}"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Rename">
											<xs:annotation>
												<xs:documentation>Изменение наименования юридического лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="FullName">
														<xs:annotation>
															<xs:documentation>Полное наименование юридического лица</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="LawForm">
														<xs:annotation>
															<xs:documentation>Организационно-правовая форма юридического лица</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="ShortName" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сокращенное наименование юридического лица (в случае, если имеется)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="FirmName" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Фирменное наименование юридического лица (в случае, если имеется)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="OGRN" hints:rus="Государственный регистрационный номер записи о создании юридического лица (ОГРН)">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="[1;2;5]{1}\d{12}"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="DocData" type="krsk:EGRULDataType">
														<xs:annotation>
															<xs:documentation>Данные документа, подтверждающего факт внесения соответствующих изменений в единый государственный реестр юридических лиц</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="INN" hints:rus="ИНН юридического лица">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{8}"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ChangeOrgAddress">
											<xs:annotation>
												<xs:documentation>Изменение адреса места нахождения юридического лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="NewAddress" type="krsk:AddressType">
														<xs:annotation>
															<xs:documentation>Адрес места нахождения юридического лица</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DocData" type="krsk:EGRULDataType">
														<xs:annotation>
															<xs:documentation>Данные документа, подтверждающего факт внесения соответствующих изменений в единый государственный реестр юридических лиц</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="AnotherAddress">
											<xs:annotation>
												<xs:documentation>Осуществление лицензируемого вида деятельности по адресу, не указанному в лицензии</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="WorksInfo" type="krsk:WorkInfoType" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Информация о новых адресах и видах деятельности</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!--													<xs:element name="Facilities">
														<xs:annotation>
															<xs:documentation>Наличие у лицензиата на праве собственности или на ином законном основании соответствующих установленным требованиям и необходимых для осуществления деятельности по обороту наркотических средств, психотропных веществ и их прекурсоров, культивированию наркосодержащих растений оборудования, помещений и земельных участков, права на которые зарегистрированы в Едином государственном реестре прав на недвижимое имущество и сделок с ним: привести перечень документов с указанием реквизитов</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Conclusion">
