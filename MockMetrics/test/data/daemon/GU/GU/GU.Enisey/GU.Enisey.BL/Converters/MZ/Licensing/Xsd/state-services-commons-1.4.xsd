<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="urn://x-artefacts-it-ru/dob/state-services/commons/1.4" xmlns:hints="urn://x-artefacts-it-ru/dob/xml-editor/rendering-hints/1.3" targetNamespace="urn://x-artefacts-it-ru/dob/state-services/commons/1.4" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--==========================================  Заготовки для структурированных типов  ==========================================-->
	<xs:group name="PhysicalPersonQualifiedName-ModelGroup">
		<xs:annotation>
			<xs:documentation>ФИО физического лица.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FamilyName" type="tns:RussianWord-100-Type" hints:rus="Фамилия" hints:userHint="Только русские буквы, до 100 символов"/>
			<xs:element name="FirstName" type="tns:RussianWord-100-Type" hints:rus="Имя" hints:userHint="Только русские буквы, до 100 символов"/>
			<xs:element name="Patronymic" type="tns:RussianWord-100-Type" minOccurs="0" hints:rus="Отчество" hints:userHint="Только русские буквы, до 100 символов"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="ApplicantPhysicalPersonQualifiedName-ModelGroup">
		<xs:annotation>
			<xs:documentation>ФИО заявителя - физического лица.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FamilyName" type="tns:RussianWord-100-Type" hints:businessMeaning="userFamilyName" hints:rus="Фамилия" hints:userHint="Только русские буквы, до 100 символов"/>
			<xs:element name="FirstName" type="tns:RussianWord-100-Type" hints:businessMeaning="userFirstName" hints:rus="Имя" hints:userHint="Только русские буквы, до 100 символов"/>
			<xs:element name="Patronymic" type="tns:RussianWord-100-Type" minOccurs="0" hints:businessMeaning="userPatronymic" hints:rus="Отчество" hints:userHint="Только русские буквы, до 100 символов"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="PhysicalPersonIdentificationInformation-ModelGroup">
		<xs:annotation>
			<xs:documentation>Установочные данные физического лица: ФИО + дата и место рождения.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="tns:PhysicalPersonQualifiedName-ModelGroup"/>
			<xs:element name="BirthDate" type="xs:date" hints:rus="Дата рождения" hints:dateRestriction="past" hints:userHint="Дата должна быть не более текущей"/>
			<xs:element name="BirthPlace" type="tns:string-500" hints:rus="Место рождения" hints:widgetTtpe="textArea" hints:userHint="Место рождения. Пример: город Богородск, Нижегородская область"/>
		</xs:sequence>
	</xs:group>
	<!--==========================================  Структурированные типы  ==========================================-->
	<!-- этот тип не используется в схемах по 14 услугам-->
	<xs:complexType name="Address-Type">
		<xs:annotation>
			<xs:documentation>
                Адрес, без почтового индекса.
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CityLocality" type="tns:string-200" hints:rus="Город/Населенный пункт"/>
			<xs:element name="Street" type="tns:string-200" hints:rus="Улица"/>
			<xs:element name="ApartmentBlock" type="tns:string-10" hints:rus="Дом"/>
			<xs:element name="Building" type="tns:string-10" minOccurs="0" hints:rus="Корпус"/>
			<xs:element name="Housing" type="tns:string-10" minOccurs="0" hints:rus="Строение"/>
			<xs:element name="Apartment" type="tns:string-10" minOccurs="0" hints:rus="Квартира"/>
		</xs:sequence>
	</xs:complexType>
	<!-- этот тип не используется в схемах по 14 услугам-->
	<xs:complexType name="PostalAddress-Type">
		<xs:annotation>
			<xs:documentation>
                Адрес, с почтовым индексом.
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="tns:SixDigitString-Type" minOccurs="0" hints:rus="Почтовый индекс"/>
			<xs:element name="CityLocality" type="tns:string-200" hints:rus="Город/Населенный пункт"/>
			<xs:element name="Street" type="tns:string-200" hints:rus="Улица"/>
			<xs:element name="ApartmentBlock" type="tns:string-10" hints:rus="Дом"/>
			<xs:element name="Building" type="tns:string-10" minOccurs="0" hints:rus="Корпус"/>
			<xs:element name="Housing" type="tns:string-10" minOccurs="0" hints:rus="Строение"/>
			<xs:element name="Apartment" type="tns:string-10" minOccurs="0" hints:rus="Квартира"/>
		</xs:sequence>
	</xs:complexType>
	<!-- этот тип адреса сейчас используется в схемах по 14 услугам-->
	<xs:complexType name="RequiredIndexAddress-Type">
		<xs:annotation>
			<xs:documentation>Тип: Адрес с обязательным почтовым индексом</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="tns:SixDigitString-Type" hints:rus="Индекс" hints:userHint="6 цифр. Пример: 450098"/>
			<xs:element name="Region" type="tns:TimelineUnawareClassifierValue-Type" form="qualified" hints:classifierName="RFRegions" hints:rus="Регион" hints:default="24" hints:userHint="Пример: Красноярский край"/>
			<xs:element name="District" type="tns:string-200" minOccurs="0" hints:rus="Район" hints:userHint="Пример: Усть-каменский"/>
			<xs:element name="CityLocality" type="tns:string-200" hints:rus="Город/Населенный пункт" hints:userHint="Город или населенный пункт. Пример: Красноярск"/>
			<xs:element name="StreetText" type="tns:string-100" minOccurs="0" hints:rus="Улица" hints:userHint="Пример: Осенний бульвар"/>
			<xs:element name="ApartmentBlock" type="tns:string-10" minOccurs="0" hints:rus="Номер дома" hints:userHint="Пример: 15"/>
			<xs:element name="Building" type="tns:string-10" minOccurs="0" hints:rus="Корпус/Строение" hints:userHint="Пример: 2А"/>
			<xs:element name="Apartment" type="tns:string-10" minOccurs="0" hints:rus="Квартира/Офис" hints:userHint="Пример: 121"/>
		</xs:sequence>
	</xs:complexType>
	<!-- этот тип адреса сейчас используется в схемах по 14 услугам-->
	<xs:complexType name="OptionalIndexAddress-Type">
		<xs:annotation>
			<xs:documentation>Тип: Адрес с необязательным почтовым индексом</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="tns:SixDigitString-Type" minOccurs="0" hints:rus="Индекс" hints:userHint="6 цифр. Пример: 450098"/>
			<xs:element name="Region" type="tns:TimelineUnawareClassifierValue-Type" form="qualified" hints:classifierName="RFRegions" hints:rus="Регион" hints:default="24" hints:userHint="Пример: Красноярский край"/>
			<xs:element name="District" type="tns:string-200" minOccurs="0" hints:rus="Район" hints:userHint="Пример: Усть-каменский"/>
			<xs:element name="CityLocality" type="tns:string-200" hints:rus="Город/Населенный пункт" hints:userHint="Город или населенный пункт. Пример: Красноярск"/>
			<xs:element name="StreetText" type="tns:string-100" minOccurs="0" hints:rus="Улица" hints:userHint="Пример: Осенний бульвар"/>
			<xs:element name="ApartmentBlock" type="tns:string-10" minOccurs="0" hints:rus="Номер дома" hints:userHint="Пример: 15"/>
			<xs:element name="Building" type="tns:string-10" minOccurs="0" hints:rus="Корпус/Строение" hints:userHint="Пример: 2А"/>
			<xs:element name="Apartment" type="tns:string-10" minOccurs="0" hints:rus="Квартира/Офис" hints:userHint="Пример: 121"/>
		</xs:sequence>
	</xs:complexType>
	<!-- этот тип не используется в схемах по 14 услугам-->
	<xs:complexType name="AddressRF-Type">
		<xs:annotation>
			<xs:documentation>Адрес РФ, с почтовым индексом.
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="tns:SixDigitString-Type" minOccurs="0" hints:rus="Почтовый индекс"/>
			<xs:element name="Region" type="tns:string-200" hints:rus="Регион"/>
			<xs:element name="District" type="tns:string-200" minOccurs="0" hints:rus="Район"/>
			<xs:element name="CityLocality" type="tns:string-200" hints:rus="Город/Населенный пункт"/>
			<xs:element name="Street" type="tns:string-200" hints:rus="Улица"/>
			<xs:element name="ApartmentBlock" type="tns:string-10" hints:rus="Дом"/>
			<xs:element name="Building" type="tns:string-10" minOccurs="0" hints:rus="Корпус"/>
			<xs:element name="Housing" type="tns:string-10" minOccurs="0" hints:rus="Строение"/>
			<xs:element name="Apartment" type="tns:string-10" minOccurs="0" hints:rus="Квартира"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LocationRosReestrKrsk-Type">
		<xs:annotation>
			<xs:documentation>
				Тип: Адрес объекта недвижимости для Красноярского края
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="tns:SixDigitString-Type" form="qualified" minOccurs="0" hints:rus="Индекс" hints:userHint="6 цифр. Пример: 105122"/>
			<xs:element name="Region" hints:rus="Регион" hints:default="Красноярский край">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Красноярский край"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AreaName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Район" hints:userHint="Пример: Канский"/>
			<xs:element name="CityName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Город" hints:userHint="Пример: Канск"/>
			<xs:element name="CityDistrictName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Городской район" hints:userHint="Пример: 5й квартал"/>
			<xs:element name="SovietVillageName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Сельсовет" hints:userHint="Пример: Садовое товарищество Красного октября"/>
			<xs:element name="NasPunktName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Населенный пункт" hints:userHint="Пример: Быково"/>
			<xs:element name="StreetName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Улица" hints:userHint="Пример: Осенний бульвар"/>
			<xs:element name="House" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Номер дома" hints:userHint="Пример: 15"/>
			<xs:element name="Building" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Корпус" hints:userHint="Пример: 2"/>
			<xs:element name="Structure" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Строение" hints:userHint="Пример: 1"/>
			<xs:element name="Apartment" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Номер квартиры" hints:userHint="Пример: 121"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LocationRosReestr-Type">
		<xs:annotation>
			<xs:documentation>
				Тип: Адрес объекта недвижимости
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="tns:SixDigitString-Type" form="qualified" minOccurs="0" hints:rus="Индекс" hints:userHint="6 цифр. Пример: 105122"/>
			<xs:element name="Region" type="tns:TimelineUnawareClassifierValue-Type" form="qualified" hints:classifierName="RFRegions" hints:rus="Регион" hints:userHint="Пример: Алтайский край"/>
			<xs:element name="AreaName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Район" hints:userHint="Пример: Усть-каменский"/>
			<xs:element name="CityName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Город" hints:userHint="Пример: Москва"/>
			<xs:element name="CityDistrictName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Городской район" hints:userHint="Пример: 5й квартал"/>
			<xs:element name="SovietVillageName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Сельсовет" hints:userHint="Пример: Садовое товарищество Красного октября"/>
			<xs:element name="NasPunktName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Населенный пункт" hints:userHint="Пример: Быково"/>
			<xs:element name="StreetName" type="tns:string-250" form="qualified" minOccurs="0" hints:rus="Улица" hints:userHint="Пример: Осенний бульвар"/>
			<xs:element name="House" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Номер дома" hints:userHint="Пример: 15"/>
			<xs:element name="Building" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Корпус" hints:userHint="Пример: 2"/>
			<xs:element name="Structure" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Строение" hints:userHint="Пример: 1"/>
			<xs:element name="Apartment" type="tns:string-10" form="qualified" minOccurs="0" hints:rus="Номер квартиры" hints:userHint="Пример: 121"/>
			<xs:element name="Note" type="tns:string-500" form="qualified" minOccurs="0" hints:widgetType="textArea" hints:rus="Неформализованное описание" hints:userHint="Пример: 250 метров на северо-запад от деревни Алешкино Нудольского р-на Московской области"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimelineAwareClassifierValue-Type">
		<xs:annotation>
			<xs:documentation>
				Справочное значение для справочников, зависящих от времени. 
				Зависимость от времени может выражаться в следующем:
				1. Текст, соответствующий коду справочного значения, меняется со временем, 
				   и эти изменения юридически значимы, т.е. при показе документа нужно показать
				   текст, соответствующий конкретному моменту времени.
				2. Справочное значение может быть объявлено устаревшим, а его код передан 
				   другому справочному значению. Пример - КЛАДР.
				   
				N.B. У элемента этого типа должен быть задан атрибут hints:classifierName.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="relevanceDate" type="xs:date" hints:speciallyTreatedDataItem="classifierValueRelevanceDate"/>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="code" type="tns:string-50" hints:speciallyTreatedDataItem="classifierCode"/>
				<xs:element name="displayText" type="tns:string-500" hints:speciallyTreatedDataItem="classifierDisplayText"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimelineUnawareClassifierValue-Type">
		<xs:annotation>
			<xs:documentation>
				Справочное значение для справочников, не зависящих от времени. 
				   
				N.B. У элемента этого типа должен быть задан атрибут hints:classifierName.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="code" type="tns:string-50" hints:speciallyTreatedDataItem="classifierCode"/>
				<xs:element name="displayText" type="tns:string-500" hints:speciallyTreatedDataItem="classifierDisplayText"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Document-Type" hints:rus="Документ, удостоверяющий личность">
		<xs:annotation>
			<xs:documentation>Документ, удостоверяющий личность</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice hints:rus="Тип документа, удостоверяющего личность" hints:default="PassportRF">
				<xs:element name="PassportRF" type="tns:PassportRFDocument" hints:rus="Паспорт (Россия)" hints:userHint="Паспорт гражданина РФ, действующий на территории РФ с 1 октября 1997 года"/>
				<xs:element name="ResidencePermit" type="tns:NotRestrictedDocument" hints:rus="Вид на жительство" hints:userHint="Вид на жительство в РФ"/>
				<xs:element name="DriverLicense" type="tns:DriverLicenseDocument" hints:rus="Водительское удостоверение"/>
				<xs:element name="MilitaryTicket" type="tns:MilitaryTicketDocument" hints:rus="Военный билет" hints:userHint="Военный билет для солдат, матросов, сержантов и старшин, проходящих военную службу по призыву или контракту"/>
				<xs:element name="AbroadPassport" type="tns:AbroadPassportDocument" hints:rus="Загранпаспорт" hints:userHint="Паспорт, удостоверяющий личность гражданина РФ за пределами РФ, образца 1997 года"/>
				<xs:element name="PassportUSSR" type="tns:PassportUSSRDocument" hints:rus="Паспорт СССР" hints:userHint="Паспорт, удостоверяющий личность гражданина СССР и действующий на территории РФ до 31 декабря 2003 года"/>
				<xs:element name="ForeignPassport" type="tns:NotRestrictedDocument" hints:rus="Паспорт иностранного гражданина" hints:userHint="Заграничный паспорт для постоянно проживающих за границей граждан, которые временно находятся на территории РФ"/>
				<xs:element name="SailorTicket" type="tns:SailorTicketDocument" hints:rus="Паспорт моряка" hints:userHint="Паспорт моряка (удостоверение личности гражданина, работающего на судах заграничного плавания или на иностранных судах) образца 1997 г."/>
				<xs:element name="BirthCertificate" type="tns:PassportUSSRDocument" hints:rus="Свидетельство о рождении" hints:userHint="Для лиц, не достигших 16-летнего (с 01.10.97 г. -14-летнего) возраста"/>
				<xs:element name="ReleaseCertificate" type="tns:NotRestrictedDocument" hints:rus="Справка об освобождении" hints:userHint="Для лиц, освободившихся из мест лишения свободы"/>
				<xs:element name="LossPassportCertificate" type="tns:NotRestrictedDocument" hints:rus="Справка об утере паспорта" hints:userHint="Справка об утере паспорта по форме 24П"/>
				<xs:element name="Form9Certificate" type="tns:NotRestrictedDocument" hints:rus="Справка формы 9" hints:userHint="Справка о регистрации"/>
				<xs:element name="IdentityCard" type="tns:NotRestrictedDocument" hints:rus="Удостоверение личности" hints:userHint="Временное удостоверение личности гражданина РФ по форме 2П"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NotRestrictedDocument">
		<xs:annotation>
			<xs:documentation>Документ с нерегламентированными серией и номером</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Series" type="tns:string-20" minOccurs="0" hints:readOnly="true" hints:rus="Серия" hints:userHint="Серия документа"/>
			<xs:element name="Number" hints:rus="Номер" hints:businessMeaning="userIdentityDocumentNumber" hints:userHint="Не более 25 символов (может содержать любые буквы и цифры). Пример: 647880. ">
				<xs:simpleType hints:userHint="Любые буквы и цифры, до 25 символов">
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9а-яА-ЯA-Za-z]{1,25}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Date" type="xs:date" hints:dateRestriction="past" hints:rus="Дата выдачи" hints:businessMeaning="userIdentityDocumentIssueDate" hints:userHint="Дата должна быть не более текущей"/>
			<xs:element name="IssueOrgan" type="tns:string-200" hints:rus="Кем выдан" hints:businessMeaning="userIdentityDocumentIssuer" hints:userHint="Орган, выдавший документ. Пример: ОВД Раменки г. Москвы"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DriverLicenseDocument">
		<xs:annotation>
			<xs:documentation>Водительское удостоверение</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Series" hints:rus="Серия" hints:businessMeaning="userIdentityDocumentSeries" hints:userHint="Две цифры и две русские заглавные буквы для водительского удостоверения, полученного до 1 марта 2011 г., или четыре цифры для водительского удостоверения, полученного после 1 марта 2011 г. Пример: 44АА или 4403">
				<xs:simpleType hints:userHint="Две цифры и две русские заглавные буквы для водительского удостоверения, полученного до 1 марта 2011 г., или четыре цифры для водительского удостоверения, полученного после 1 марта 2011 г. Пример: 44АА или 4403">
					<xs:restriction base="xs:string">
						<xs:length value="4"/>
						<xs:pattern value="[0-9]{2}([0-9]{2})|([А-Я]{2})"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
